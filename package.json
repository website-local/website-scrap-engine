{
  "name": "website-scrap-engine",
  "version": "0.3.2",
  "description": "Configurable website scraper in typescript",
  "main": "lib/index",
  "engines": {
    "node": ">=12.16.0"
  },
  "scripts": {
    "clean": "rimraf lib/*",
    "tsc": "tsc",
    "lint": "eslint --fix src test",
    "test": "npm run lint && jest",
    "build": "npm run lint && npm run tsc && npm run copy",
    "copy": "node copy-src.js",
    "prepack": "npm run clean && npm run build",
    "postshrinkwrap": "node package-lock-resolved.js"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.5",
    "css-url-parser": "^1.1.3",
    "got": "^11.8.0",
    "log4js": "^6.3.0",
    "mkdirp": "^1.0.4",
    "p-queue": "^6.6.2",
    "srcset": "^3.0.0",
    "urijs": "^1.19.5"
  },
  "jest": {
    "preset": "ts-jest"
  },
  "devDependencies": {
    "@types/jest": "^26.0.19",
    "@types/mkdirp": "^1.0.1",
    "@types/node": "^14.14.20",
    "@types/urijs": "^1.19.13",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "eslint": "^7.17.0",
    "jest": "^26.6.3",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3"
  },
  "files": [
    ".editorconfig",
    "tsconfig.json",
    "lib",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/website-local/website-scrap-engine.git"
  },
  "keywords": [
    "typescript",
    "website",
    "scraper",
    "download"
  ],
  "author": "myfreeer",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/myfreeer/website-scrap-engine/issues"
  },
  "homepage": "https://github.com/myfreeer/website-scrap-engine#readme"
}
